import numpy as np
import matplotlib.pyplot as plt
import pandas as pd

dataset = pd.read_csv('50_Startups_dataset.csv')
dataset.head()

X = dataset.iloc[:,:-1].values
Y = dataset.iloc[:,-1].values 
print("Independent Variables : ")
print(X[:5,:])
print("\n\nDependent Variables : ")
print(Y[:5])

dataset.isnull().sum()

new_df = dataset.fillna(0)
new_df

new_df1 = dataset.fillna(method="ffill")
new_df1

new_df2 = dataset.dropna()
new_df2

new_df3 = dataset.dropna(how="all")
new_df3

new_df4 = dataset.replace(to_replace = np.nan,value = -99)
new_df4
